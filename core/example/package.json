{
	"name": "create-stephansama-example",
	"version": "1.2.0",
	"description": "Download an example from the @stephansama/packages examples",
	"keywords": [
		"create-example"
	],
	"homepage": "https://packages.stephansama.info/api/create-stephansama-example",
	"repository": {
		"type": "git",
		"url": "https://github.com/stephansama/packages",
		"directory": "core/example"
	},
	"license": "MIT",
	"author": {
		"name": "Stephan Randle",
		"email": "stephanrandle.dev@gmail.com",
		"url": "https://stephansama.info"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": "./cli.mjs",
	"files": [
		"./dist",
		"./cli.mjs"
	],
	"scripts": {
		"build": "pnpm --workspace-root run scripts:generate-examples && tsup && attw --pack",
		"dev": "pnpm --workspace-root run scripts:generate-examples && tsup --watch",
		"lint": "eslint ./src/ --pass-on-no-patterns --no-error-on-unmatched-pattern",
		"typedoc": "pnpm run generate"
	},
	"dependencies": {
		"@bluwy/giget-core": "^0.1.2",
		"@clack/prompts": "^0.11.0"
	},
	"devDependencies": {
		"tsup": "catalog:"
	},
	"packageManager": "pnpm@10.11.0",
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
