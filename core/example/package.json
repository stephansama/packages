{
	"name": "create-stephansama-example",
	"version": "1.3.3",
	"description": "Download an example from the @stephansama/packages examples",
	"keywords": [
		"create-example"
	],
	"homepage": "https://packages.stephansama.info/api/create-stephansama-example",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stephansama/packages.git",
		"directory": "core/example"
	},
	"license": "MIT",
	"author": {
		"name": "Stephan Randle",
		"email": "stephanrandle.dev@gmail.com",
		"url": "https://stephansama.info"
	},
	"type": "commonjs",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": "./cli.mjs",
	"files": [
		"./dist",
		"./cli.mjs"
	],
	"scripts": {
		"prebuild": "pnpm run generate",
		"build": "tsdown",
		"postbuild": "publint",
		"predev": "pnpm run generate",
		"dev": "tsdown --watch",
		"generate": "pnpm --workspace-root run scripts:generate-examples",
		"lint": "eslint ./src/ --pass-on-no-patterns --no-error-on-unmatched-pattern",
		"typedoc": "pnpm run generate"
	},
	"dependencies": {
		"@bluwy/giget-core": "^0.1.2",
		"@clack/prompts": "^0.11.0"
	},
	"devDependencies": {
		"tsdown": "catalog:"
	},
	"packageManager": "pnpm@10.11.0",
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
