{
	"name": "@stephansama/svelte-social-share-links",
	"version": "0.0.0",
	"private": true,
	"sideEffects": [
		"**/*.css"
	],
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./wc": "./dist-js/components.iife.js"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist-js",
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"scripts": {
		"prebuild": "svelte-package",
