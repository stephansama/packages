		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tsconfig/svelte": "^5.0.5",
		"@vitest/browser": "^3.2.4",
		"@vitest/browser-playwright": "^4.0.6",
		"@vitest/coverage-v8": "^4.0.6",
		"playwright": "^1.56.1",
		"storybook": "^10.0.2",
		"svelte": "^5.39.6",
		"svelte-check": "^4.3.2",
		"typescript": "catalog:",
		"vite": "^7.1.7"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	}
