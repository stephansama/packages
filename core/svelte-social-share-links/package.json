{
	"name": "@stephansama/svelte-social-share-links",
	"version": "1.1.0",
	"private": false,
	"description": "Svelte/Web component to share the current url with various social media providers",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stephansama/packages.git",
		"directory": "core/svelte-social-share-links"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./wc": "./dist-js/components.iife.js",
		"./networks": {
			"types": "./dist/networks.d.ts",
			"require": "./dist/networks.js"
		},
		"./networks.json": "./dist/networks.json"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist-js",
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"scripts": {
		"prebuild": "svelte-package",
		"build": "vite -c ./vite.webcomponent.config.ts build",
		"postbuild": "publint",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"dev": "svelte-package --watch",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "catalog:storybook",
		"@storybook/addon-a11y": "catalog:storybook",
		"@storybook/addon-docs": "catalog:storybook",
		"@storybook/addon-svelte-csf": "catalog:storybook",
		"@storybook/svelte-vite": "catalog:storybook",
		"@sveltejs/package": "catalog:svelte",
		"@sveltejs/vite-plugin-svelte": "catalog:svelte",
		"@tsconfig/svelte": "catalog:svelte",
		"storybook": "catalog:storybook",
		"svelte": "catalog:svelte",
		"svelte-check": "catalog:svelte",
		"typescript": "catalog:",
		"vite": "catalog:"
	},
	"peerDependencies": {
		"svelte": ">=5"
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"readme": "./README.md"
}
