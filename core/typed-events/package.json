{
	"name": "@stephansama/typed-events",
	"version": "2.0.4",
	"description": "Typed events store using standard schema",
	"keywords": [
		"typed-events",
		"standard-schema",
		"validation",
		"events",
		"strict"
	],
	"homepage": "https://packages.stephansama.info/api/@stephansama/typed-events",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stephansama/packages.git",
		"directory": "core/typed-events"
	},
	"license": "MIT",
	"author": {
		"name": "Stephan Randle",
		"email": "stephanrandle.dev@gmail.com",
		"url": "https://stephansama.info"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist/"
	],
	"scripts": {
		"build": "tsdown",
		"postbuild": "publint",
		"dev": "tsdown --watch",
		"lint": "eslint ./src/ --pass-on-no-patterns --no-error-on-unmatched-pattern"
	},
	"devDependencies": {
		"@standard-schema/spec": "^1.0.0",
		"tsdown": "catalog:",
		"zod": "catalog:"
	},
	"packageManager": "pnpm@10.11.0",
	"publishConfig": {
		"access": "public",
		"provenance": true
