{
	"name": "@stephansama/remark-asciinema",
	"type": "module",
	"readme": "./README.md",
	"license": "MIT",
	"version": "1.0.6",
	"homepage": "https://stephansama-pkgs.pages.dev/modules/_stephansama_remark-asciinema",
	"description": "A remark plugin that transforms Asciinema links into embedded players or screenshots.",
	"keywords": [
		"asciinema",
		"remark",
		"remark-plugin"
	],
	"author": {
		"email": "stephanrandle.dev@gmail.com",
		"name": "Stephan Randle",
		"url": "https://stephanrandle.netlify.app/"
	},
	"repository": {
		"url": "https://github.com/stephansama/packages",
		"type": "git",
		"directory": "packages/remark-asciinema"
	},
	"scripts": {
		"build": "tsup",
		"lint": "eslint ./src/",
		"prepublishOnly": "tsup",
		"dev": "tsup --watch",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest run --ui"
	},
	"files": [
		"dist",
		"package.json"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"devDependencies": {
		"@config/tsconfig": "workspace:*",
		"remark": "^15.0.1",
		"tsup": "catalog:",
		"unist-builder": "^4.0.0",
		"unist-util-is": "^6.0.0",
		"unist-util-visit": "^5.0.0"
