{
	"$schema": "https://typedoc.org/schema.json",
	"out": "./dist",
	"name": "@stephansama packages",
	"readme": "./README.md",
	"customCss": "./typedoc.css",
	"navigation": true,
	"excludeExternals": true,
	"skipErrorChecking": true,
	"jsDocCompatibility": true,
	"darkHighlightTheme": "catppuccin-mocha",
	"lightHighlightTheme": "catppuccin-latte",
	"entryPointStrategy": "packages",
	"entryPoints": ["packages/*"],
	"projectDocuments": ["./.github/*"],
	"exclude": ["**/ISSUE_TEMPLATE/**"],
	"navigationLinks": {
		"Author": "https://stephanrandle.netlify.app",
		"Blog": "https://madprofessorblog.org",
		"GitHub": "https://github.com/stephansama/packages"
	},
	"highlightLanguages": [
		"bash",
		"console",
		"css",
		"html",
		"javascript",
		"json",
		"jsonc",
		"json5",
		"yaml",
		"tsx",
		"typescript",
		"markdown"
	]
}
